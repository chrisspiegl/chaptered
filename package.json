{
  "type": "module",
  "name": "chaptered",
  "version": "0.0.1",
  "description": "Convert markers from different programs to shownotes or chapteres.",
  "license": "MIT",
  "repository": "chrisspiegl/chaptered",
  "author": {
    "name": "Chris Spiegl",
    "email": "chris@chrisspiegl.com",
    "url": "ChrisSpiegl.com"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "xo && ava -v",
    "ava": "ava -v --watch",
    "cu": "ncu",
    "cu:fix": "ncu -u",
    "format": "prettier . --write --ignore-unknown"
  },
  "files": [
    "index.js",
    "Chaptered.js"
  ],
  "keywords": [
    "marker",
    "transform",
    "shownotes",
    "adobe",
    "audition",
    "premierepro",
    "podcast",
    "youtube",
    "chapters",
    "cue markers"
  ],
  "dependencies": {
    "Buffer": "^0.0.0",
    "camelcase": "^6.2.1",
    "date-fns": "^2.27.0",
    "duration": "^0.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "papaparse": "^5.3.1",
    "pupa": "^3.1.0",
    "slugify": "^1.6.4",
    "smpte-timecode": "^1.2.3",
    "unraw": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "chardet": "^1.4.0",
    "npm-check-updates": "^12.0.5",
    "xo": "^0.47.0"
  }
}
